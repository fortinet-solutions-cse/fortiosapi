#!/home/ubuntu/cloudify/my_virtualenv/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'cloudify-plugins-common==3.4','console_scripts','ctx'
__requires__ = 'cloudify-plugins-common==3.4'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('cloudify-plugins-common==3.4', 'console_scripts', 'ctx')()
    )
